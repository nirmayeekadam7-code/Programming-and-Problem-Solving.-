
day = int(input())
month = int(input())
year = int(input())


def is_leap_year(year):
	if (year % 400 == 0) or (year % 4 == 0 and year % 100 != 0):
        return True
    return False

# Function to get number of days in a month
def get_days_in_month(month, year):
    if month in [1, 3, 5, 7, 8, 10, 12]:
        return 31
    elif month in [4, 6, 9, 11]:
        return 30
    elif month == 2:
        if is_leap_year(year):
            return 29
        else:
            return 28
    else:
        return -1  # Invalid month

# Validate year
if year <= 0:
    print("Invalid Date")

# Validate month
# Validate month
elif month < 1 or month > 12:
    print("Invalid Date")

else:
    max_days = get_days_in_month(month, year)

    # Validate day
    if day < 1 or day > max_days:
        print("Invalid Date")
    else:
        # Increment date
        day += 1

        if day > max_days:
            day = 1
            month += 1

            if month > 12:
                month = 1
                year += 1

        # Print in DD-MM-YYYY format
        print(f"{day:02d}-{month:02d}-{year:04d}")



